{
    "manifest_version": 2,

    "name": "Badger",
    "description": "Add custom badges to Trello.",
    "version": "1.0",

    "content_scripts": [{
        "matches": [ "https://trello.com/*", "file:///*" ],
        "js": [ "badger.js" ],
        "css": [
            "style.css"
        ]
    }]
}
